function Array_instance_counter_type_alloc()
   local _ctx = {};
   _ctx.x = 0
   return _ctx
end

function Array_instance_foo_type_alloc()
   local i_0 = 0
   local temp
   local _ctx = {};
   while (i_0 < 2) do
      do
         temp[i_0 + 1] = Array_instance_counter_type_alloc()
         i_0 = (1 + i_0)
      end
   end
   _ctx.x = temp
   return _ctx
end

function Array_instance_counter(_ctx)
   _ctx.x = (1 + _ctx.x)
   return _ctx.x
end

function Array_instance_incr(ctx)
   ctx.x = (1 + ctx.x)
   return ctx.x
end

function Array_instance_foo(_ctx)
   local i = 0
   while (i < 2) do
      do
         Array_instance_counter(_ctx.x[i + 1])
         i = (1 + i)
      end
   end
   return _ctx.x[1 + 1].x
end

