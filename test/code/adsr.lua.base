function Adsr_do(_ctx, gate, a, d, s, r)
   local a_rate = (1. / (0.01 + (100. * a)))
   local d_rate = (1. / (0.01 + (100. * d)))
   local r_rate = (1. / (0.01 + (100. * r)))
   _ctx.out = ((0.004 * (((-_ctx.out) + _ctx.target) * _ctx.rate)) + _ctx.out)
   local bgate = (gate > 0.)
   if (_ctx.state == 0) then
      do
         if Util_edge(_ctx.inst_104a0, bgate) then
            do
               _ctx.state = 1
               _ctx.scale = (0.2 * gate)
            end
         end
         _ctx.rate = r_rate
         _ctx.target = 0.
      end
   end
   if (_ctx.state == 1) then
      do
         if (bgate == false) then
            _ctx.state = 0
         end
         if (_ctx.out > 1024.) then
            _ctx.state = 2
         end
         _ctx.rate = a_rate
         _ctx.target = 1228.8
      end
   end
   if (_ctx.state == 2) then
      do
         if (bgate == false) then
            _ctx.state = 0
         end
         _ctx.rate = d_rate
         _ctx.target = (1024. * s)
      end
   end
   return (Util_smooth(_ctx.inst_295a0, _ctx.scale) * clip((0.0009766 * _ctx.out), 0., 1.))
end

