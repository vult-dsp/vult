function Volume_smooth_type_init(_ctx)
   _ctx.x = 0.0;
end

function Volume_process_type_init(_ctx)
   Volume_smooth_type_init(_ctx.inst_1d7d3);
   _ctx.volume = 0.0;
end

function Volume_smooth(_ctx, input)
   _ctx.x = (_ctx.x + ((input - _ctx.x) * 0.005));
   return _ctx.x;
end

function Volume_process(_ctx, input)
   return (input * Volume_smooth(_ctx.inst_1d7d3, _ctx.volume));
end

function Volume_noteOn(_ctx, note, velocity, channel)
end

function Volume_noteOff(_ctx, note, channel)
end

function Volume_controlChange(_ctx, control, value, channel)
   if (control == 30) then
      _ctx.volume = (real(value) / 127.0);
   end
end

function Volume_default(_ctx)
   _ctx.volume = 0.0;
end

