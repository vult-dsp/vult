function Synth2_edge(_ctx, x)
   local v = ((_ctx.pre_x ~= x) and (_ctx.pre_x == true))
   _ctx.pre_x = x
   return v
end

function Synth2_each(_ctx, n)
   local ret = (_ctx.count == 0)
   _ctx.count = ((1 + _ctx.count) % n)
   return ret
end

function Synth2_near_zero(x)
   return (abs(x) < 0.02)
end

function Synth2_dcblock(_ctx, x0)
   local y0 = (x0 + ((-_ctx.x1) + (0.995 * _ctx.y1)))
   _ctx.x1 = x0
   _ctx.y1 = y0
   return y0
end

function Synth2_smooth(_ctx, input)
   _ctx.x = ((0.005 * (input + (-_ctx.x))) + _ctx.x)
   return _ctx.x
end

function Synth2_lpfilter(_ctx, x)
   local ret = (0.5 * (x + _ctx.pre_x))
   _ctx.pre_x = x
   return ret
end

function Synth2_pitchToRate(d)
   return (0.00019 * exp((0.05777 * d)))
end

function Synth2_pulse_train(m, phase)
   local pi_phase = (3.1416 * phase)
   local denominator1 = sin(pi_phase)
   local tmp1 = 0.
   if Synth2_near_zero(denominator1) then
      tmp1 = 1.
   else
      do
         tmp1 = sin((m * pi_phase))
         tmp1 = (tmp1 / (denominator1 * m))
      end
   end
   return tmp1
end

function Synth2_osc(_ctx, pitch, pw, wave)
   local fixed_pitch = 0.
   if (wave < 0.66667) then
      fixed_pitch = pitch
   else
      fixed_pitch = (12. + pitch)
   end
   if Synth2_change(_ctx.inst_177e5, fixed_pitch) then
      do
         _ctx.rate = Synth2_pitchToRate(fixed_pitch)
         local p = (1. / _ctx.rate)
         local maxHarmonics = floor((0.5 * p))
         _ctx.m = (1. + (2. * maxHarmonics))
      end
   end
   local shift05 = (0.5 + (0.49 * pw))
   local shift = (shift05 + _ctx.phase)
   if (shift > 1.) then
      shift = (-1. + shift)
   end
   local tmp1 = Synth2_pulse_train(_ctx.m, _ctx.phase)
   local tmp2 = Synth2_pulse_train(_ctx.m, shift)
   _ctx.phase = (_ctx.phase + _ctx.rate)
   if (_ctx.phase > 1.) then
      _ctx.phase = (-1. + _ctx.phase)
   end
   _ctx.state_pulse = clip((tmp1 + ((-tmp2) + (0.9995 * _ctx.state_pulse))), -1., 1.)
   _ctx.state_saw = clip(((0.5 * ((tmp1 + (tmp2 + (-2. * _ctx.rate))) / shift05)) + (0.9995 * _ctx.state_saw)), -1., 1.)
   _ctx.state_triang = clip(((0.9995 * _ctx.state_triang) + (2. * (_ctx.state_pulse * _ctx.rate))), -1., 1.)
   if (wave < 0.33334) then
      _ctx.output = _ctx.state_pulse
   else
      if (wave < 0.66667) then
         _ctx.output = (2. * _ctx.state_saw)
      else
         _ctx.output = (2. * ((1. + pw) * _ctx.state_triang))
      end
   end
   _ctx.output = Synth2_dcblock(_ctx.inst_24fe5, _ctx.output)
   return clip((0.25 * _ctx.output), -1., 1.)
end

function Synth2_svf_step(_ctx, input, g, q, sel)
   local low = ((g * _ctx.dband) + _ctx.dlow)
   local high = (input + ((-low) + (-(q * _ctx.dband))))
   local band = ((g * high) + _ctx.dband)
   local notch = (high + low)
   _ctx.dband = clip(band, -1., 1.)
   _ctx.dlow = clip(low, -1., 1.)
   local output = 0.
   if (sel == 0) then
      output = low
   else
      if (sel == 1) then
         output = high
      else
         if (sel == 2) then
            output = band
         else
            output = notch
         end
      end
   end
   return output
end

function Synth2_svf(_ctx, input, fc, q, sel)
   fc = clip(fc, 0., 1.)
   q = clip(q, 0., 1.)
   local fix_q = (2. + (2. * (-q)))
   if Synth2_change(_ctx.inst_177a7, fc) then
      _ctx.g = (0.5 * fc)
   end
   local x1 = Synth2_svf_step(_ctx.step, input, _ctx.g, fix_q, sel)
   local x2 = Synth2_svf_step(_ctx.step, input, _ctx.g, fix_q, sel)
   return (0.5 * (x1 + x2))
end

function Synth2_lfo(_ctx, f, gate)
   local rate = ((100. * (f * Synth2_minFixed())) + Synth2_minFixed())
   if Synth2_edge(_ctx.inst_1445f, gate) then
      _ctx.phase = 0.
   end
   if Synth2_each(_ctx.inst_2cb5f, 4) then
      _ctx.phase = (rate + _ctx.phase)
   end
   if (_ctx.phase > 1.) then
      _ctx.phase = (-1. + _ctx.phase)
   end
   return (0.5 + sin((6.28319 * _ctx.phase)))
end

function Synth2_mono_noteOn(_ctx, n)
   if (_ctx.count < 4) then
      do
         _ctx.notes[_ctx.count + 1] = n
         _ctx.pre = n
         if (_ctx.count < 4) then
            _ctx.count = (1 + _ctx.count)
         end
      end
   end
   return _ctx.pre
end

function Synth2_mono_noteOff(_ctx, n)
   local found = false
   local i = 0
   local pos = 0
   if (_ctx.count == 0) then
      return _ctx.pre
   end
   while ((i < 4) and (found == false)) do
      do
         if (_ctx.notes[i + 1] == n) then
            do
               pos = i
               found = true
            end
         end
         i = (1 + i)
      end
   end
   if found then
      do
         local k = (1 + pos)
         while (k < 4) do
            do
               _ctx.notes[(-1 + k) + 1] = _ctx.notes[k + 1]
               k = (1 + k)
            end
         end
         if (found and (_ctx.count > 0)) then
            do
               _ctx.count = (-1 + _ctx.count)
               _ctx.pre = _ctx.notes[(-1 + _ctx.count) + 1]
            end
         end
      end
   end
   return _ctx.pre
end

function Synth2_mono_isGateOn(_ctx)
   return (_ctx.count > 0)
end

function Synth2_process(_ctx, i)
   local gate = Synth2_mono_isGateOn(_ctx.monoin)
   local lfo1 = (Synth2_lfo(_ctx.inst_15fb7, _ctx.lfo_rate, gate) * _ctx.lfo_amt)
   local o1 = Synth2_osc(_ctx.inst_2e5b7, _ctx.pitch, (lfo1 + _ctx.pw), _ctx.wave)
   local amp_env = 0.
   local _if_temp_0 = 0.
   if gate then
      _if_temp_0 = 1.
   else
      _if_temp_0 = 0.
   end
   amp_env = Synth2_smooth(_ctx.inst_3dfb7, _if_temp_0)
   local output = (amp_env * Synth2_svf(_ctx.inst_4a7b7, o1, _ctx.cut, _ctx.res, 0))
   return (output * _ctx.volume)
end

function Synth2_noteOn(_ctx, note, velocity, channel)
   _ctx.pitch = real(Synth2_mono_noteOn(_ctx.monoin, note))
end

function Synth2_noteOff(_ctx, note, channel)
   _ctx.pitch = real(Synth2_mono_noteOff(_ctx.monoin, note))
end

function Synth2_controlChange(_ctx, control, value, channel)
   local value_0_1 = (0.00788 * real(value))
   local value_m1_1 = (-1. + (2. * value_0_1))
   if (control == 30) then
      _ctx.volume = value_0_1
   end
   if (control == 31) then
      _ctx.wave = value_0_1
   end
   if (control == 32) then
      _ctx.pw = value_0_1
   end
   if (control == 33) then
      _ctx.lfo_rate = value_0_1
   end
   if (control == 34) then
      _ctx.lfo_amt = value_m1_1
   end
   if (control == 35) then
      _ctx.cut = value_0_1
   end
   if (control == 36) then
      _ctx.res = value_0_1
   end
end

