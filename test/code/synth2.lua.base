function Synth2_minFixed()
   return 2e-05
end

function Synth2_change(_ctx, x)
   local v = (_ctx.pre_x ~= x)
   _ctx.pre_x = x
   return v
end

function Synth2_bchange(_ctx, x)
   local v = (_ctx.pre_x ~= x)
