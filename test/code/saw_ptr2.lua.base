      _ctx.rate = Util_cvToRate(cv)
   end
   local s1 = _ctx.rate
   local s2 = (s1 + _ctx.rate)
   _ctx.phase = (_ctx.phase + _ctx.rate)
   local out = 0.
   if (_ctx.phase > 1.) then
      _ctx.phase = (-1. + _ctx.phase)
   else
      _ctx.phase = _ctx.phase
   end
   if (_ctx.phase < s1) then
      do
         local d = (_ctx.phase / _ctx.rate)
         out = (1. + (d * (s2 + (-d))))
      end
   else
      if (_ctx.phase < s2) then
         do
            local d = (_ctx.phase / _ctx.rate)
            out = (3. + (d * (-4. + (d + s2))))
         end
      else
         out = (-1. + (2. * _ctx.phase))
      end
   end
   return out
end

