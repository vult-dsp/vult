Tanh satuation with limits around -16 to 16
*/


fun tanh_table(x) @[table(min = -24.0, max = 24.0, size = 241)] {
  return 16.0 * tanh(x / 16.0);
}

fun process(x) {
  return tanh_table(x);
}
